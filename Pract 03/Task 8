#include <iostream>

int main() {
    int a, b, c;

    std::cin >> a >> b >> c;

    if (a > b) { a = a + b; b = a - b; a = a - b; } 
    if (a > c) { a = a + c; c = a - c; a = a - c; }
    if (b > c) { b = b + c; c = b - c; b = b - c; }

    std::cout << a << " " << b << " " << c << std::endl;

   
}
