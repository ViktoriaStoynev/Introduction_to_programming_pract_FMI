#include <iostream>
#include <cmath>

int main() {
    double x, y, radius;

    std::cin >> x >> y >> radius;

    double distance = std::sqrt(x * x + y * y);

    if (distance < radius) {
        std::cout << "Inside the circle";
    }
    else if (distance == radius) {
        std::cout << "On the circle";
    }
    else {
        std::cout << "Outside the circle";
    }


}
